{
  "name": "react-announce",
  "description": "a declarative approach to writing react components",
  "main": "src/index.js",
  "scripts": {
    "test": "ava",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "tusharmath@gmail.com",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.2.0"
  },
  "peerDependencies": {
    "rx": "^4.0.7"
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "cracks": "^3.1.2",
    "cz-conventional-changelog": "^1.1.5",
    "semantic-release": "^4.3.5",
    "rx": "^4.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tusharmath/react-announce.git"
  },
  "keywords": [
    "decorators",
    "declarative",
    "react",
    "rx",
    "observable",
    "stream",
    "framework",
    "announce"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "tag": "stable"
  },
  "release": {
    "branch": "stable",
    "verifyRelease": {
      "path": "cracks",
      "paths": [
        "test",
        "package.json"
      ],
      "silent": true
    }
  }
}
